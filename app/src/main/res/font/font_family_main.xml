<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!--    100    Extra Light or Ultra Light-->
    <!--    200    Light or Thin-->
    <!--    300    Book or Demi-->
    <!--    400    Normal or Regular-->
    <!--    500    Medium-->
    <!--    600    Semibold, Demibold-->
    <!--    700    Bold-->
    <!--    800    Black, Extra Bold or Heavy-->
    <!--    900    Extra Black, Fat, Poster or Ultra Black-->

    <font
        app:font="@font/inter_extra_light"
        app:fontStyle="normal"
        app:fontWeight="100" />
    <font
        app:font="@font/inter_extra_light_italic"
        app:fontStyle="italic"
        app:fontWeight="100" />
    <font
        app:font="@font/inter_light"
        app:fontStyle="normal"
        app:fontWeight="200" />
    <font
        app:font="@font/inter_light_italic"
        app:fontStyle="italic"
        app:fontWeight="200" />
    <font
        app:font="@font/inter_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />
    <font
        app:font="@font/inter_regular_italic"
        app:fontStyle="italic"
        app:fontWeight="400" />
    <font
        app:font="@font/inter_medium"
        app:fontStyle="normal"
        app:fontWeight="500" />
    <font
        app:font="@font/inter_medium_italic"
        app:fontStyle="italic"
        app:fontWeight="500" />
    <font
        app:font="@font/inter_semi_bold"
        app:fontStyle="normal"
        app:fontWeight="600" />
    <font
        app:font="@font/inter_semi_bold_italic"
        app:fontStyle="italic"
        app:fontWeight="600" />
    <font
        app:font="@font/inter_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />
    <font
        app:font="@font/inter_bold_italic"
        app:fontStyle="italic"
        app:fontWeight="700" />
    <font
        app:font="@font/inter_extra_bold"
        app:fontStyle="normal"
        app:fontWeight="800" />
    <font
        app:font="@font/inter_extra_bold_italic"
        app:fontStyle="italic"
        app:fontWeight="800" />
</font-family>